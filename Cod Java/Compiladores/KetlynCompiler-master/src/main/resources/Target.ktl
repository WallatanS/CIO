ketlyn::init
    cd::var inteiro;
    cd::var soma;

    ktl::write ("Digite um numero");
    ktl::read (inteiro);

    soma = 2;
    soma = inteiro/3;
    ktl::write ("soma");

    cd::if (inteiro < 3)
        ktl::write ("inteiro e menor que 3");
    cd::else
        ktl::write ("inteiro e maior que 3");
    cd::endif

    cd::loop (inteiro < 10)
        cd::write ("ainda nao chegou no 10");
        inteiro = inteiro + 1;
    cd::endloop
ketlyn::close